# EASY-INSTALL-ENTRY-SCRIPT: 'ase','console_scripts','ase-info'
__requires__ = 'ase'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ase', 'console_scripts', 'ase-info')()
    )
